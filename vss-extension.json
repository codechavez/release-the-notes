{
    "manifestVersion": 1,
    "id": "build-release-the-notes-task",
    "name": "Release the Notes",
    "version": "0.1.86",
    "publisher": "ViFrankenstein",
    "targets": [
        {
            "id": "Microsoft.VisualStudio.Services"
        }
    ],
    "description": "Create release note inside your project's wiki. Add release notes step to your Azure pipeline",
    "categories": [
        "Azure Pipelines"
    ],
    "icons": {
        "default": "images/icon.png",
        "note": "images/notes_screen.png",
        "config":"images/note_screen_configs.png"
    },
    "tags": [
        "CI/CD",
        "Notes",
        "Release Notes",
        "Pipeline",
        "WorkItems"
    ],
    "content": {
        "details": {
            "path": "README.md"
        },
        "license": {
            "path": "LICENSE"
        }
    },
    "links":{
        "home": {
            "uri": "https://codechavez.com"
        },
        "issues": {
            "uri": "https://github.com/codechavez/release-the-notes/issues"
        }
    },
    "repository": {
        "type": "git",
        "uri": "https://github.com/codechavez/release-the-notes"
    },
    "files": [
        {
            "path": "src"
        }
    ],
    "contributions": [
        {
            "id": "custom-build-release-the-notes-task",
            "type": "ms.vss-distributed-task.task",
            "targets": [
                "ms.vss-distributed-task.tasks"
            ],
            "properties": {
                "name": "src"
            }
        }
    ]
}